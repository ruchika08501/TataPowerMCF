<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="sap.ui.demo.nav.TATAPOWER..controller.application_form" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page>
				<content>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.HeaderMain" type="XML"/>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.HeaderMenu" type="XML"/>
					<html:div class="container-fluid">
						<html:div class="row">
							<html:div class="col-md-3">
								<Tree id="Tree" items="{path: '/'}">
									<CustomTreeItem>
										<FlexBox alignItems="Start" width="100%">
											<items >
												<FlexBox >
													<Link text="{text}" press="handleButtonPress"/>
												</FlexBox>
											</items>
										</FlexBox>
									</CustomTreeItem>
								</Tree>
							</html:div>
							<html:div class="col-md-9">
								<Panel>
									<html:div>
										<html:div class=" col-md-8">
											<html:h3>Aplication Forms</html:h3>
											<html:div class="panel panel-default">
												<html:div class="panel-body">
													<html:div class="app">
														<html:div class=" col-md-10">
															<html:a href="#">
																<html:h4>Power Supply Application form</html:h4>
															</html:a>
														</html:div>
														<html:div class=" col-md-2">
															<html:i class="fa fa-angle-right" style="font-size:38px;color:#333333"></html:i>
														</html:div>
													</html:div>
												</html:div>
											</html:div>
											<html:div class="row">
												<html:div class=" col-md-6">
													<html:div class="panel panel-default">
														<html:div class="panel-body pdfviewer">
															<html:a href="pdf/LN.03.01-Application form 04.pdf" target="_blank">
																<html:i class="fa fa-file-pdf-o" style=" font-size:50px; color:red; text-align:center"></html:i>
															</html:a>
														</html:div>
														<html:div class="panel-footer">
															<html:h4>Power Supply Agreement</html:h4>
														</html:div>
													</html:div>
												</html:div>
												<html:div class=" col-md-6">
													<html:div class="panel panel-default">
														<html:div class="panel-body pdfviewer">
															<html:a href="pdf/LN.03.01-Application form 04.pdf" target="_blank">
																<html:i class="fa fa-file-pdf-o" style=" font-size:50px; color:red; text-align:center"></html:i>
															</html:a>
														</html:div>
														<html:div class="panel-footer">
															<html:h4>Lease Deed</html:h4>
														</html:div>
													</html:div>
												</html:div>
											</html:div>
										</html:div>
									</html:div>
								</Panel>
							</html:div>
						</html:div>
					</html:div>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.Footer" type="XML"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>