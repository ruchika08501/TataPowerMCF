<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="sap.ui.demo.nav.TATAPOWER..controller.FAQ"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page>
				<content>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.HeaderMain" type="XML"/>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.HeaderMenu" type="XML"/>
					<html:div class="container-fluid">
						<html:div class="row">
							<html:div class="col-md-3">
								<Tree id="Tree" items="{path: '/'}">
									<CustomTreeItem>
										<FlexBox alignItems="Start" width="100%">
											<items >
												<FlexBox >
													<Link text="{text}" press="handleButtonPress"/>
												</FlexBox>
											</items>
										</FlexBox>
									</CustomTreeItem>
								</Tree>
							</html:div>
							<html:div class="col-md-9">
								<Panel>
									<html:div>
										<html:div class=" col-md-4">
											<html:h3>Procedure And Guidelines</html:h3>
											<html:div class="panel panel-default">
												<html:div class="panel-body pdfviewer">
													<html:a href="pdf/LN.03.05-Procedure and Guidelines.pdf" target="_blank">
														<html:i class="fa fa-file-pdf-o" style=" font-size:50px; color:red; text-align:center"></html:i>
													</html:a>
												</html:div>
												<html:div class="panel-footer">Procedure and Guidelines</html:div>
											</html:div>
										</html:div>
									</html:div>
								</Panel>
							</html:div>
						</html:div>
					</html:div>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.Footer" type="XML"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>