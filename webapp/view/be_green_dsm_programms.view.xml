<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="sap.ui.demo.nav.TATAPOWER..controller.be_green_dsm_programms" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page>
				<content>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.HeaderMain" type="XML"/>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.HeaderMenu" type="XML"/>
					<html:div class="container-fluid">
						<html:div class="row">
							<html:div class="col-md-3">
								<Tree id="Tree" items="{path: '/'}">
									<CustomTreeItem>
										<FlexBox alignItems="Start" width="100%">
											<items >
												<FlexBox >
													<Link text="{text}" press="handleButtonPress"/>
												</FlexBox>
											</items>
										</FlexBox>
									</CustomTreeItem>
								</Tree>
							</html:div>
							<html:div class="col-md-9">
								<Panel>
									<html:div>
										<html:div class="container-fluid">
											<html:div class="go-green_intro">
												<html:div class="intro_head">
													<html:h4>DSM Programms</html:h4>
												</html:div>
												<html:div class="intro_img">
													<html:image src="images/be_green_banner.png" alt="be_green_banner" class="img-responsive"></html:image>
												</html:div>
												<html:div class="prog_consumers">
													<html:div class="row">
														<html:div class="col-sm-10 col-sm-offset-1">
															<html:div class="row">
																<html:div class="col-sm-6">
																	<html:h5>Programmes for Residential Consumers</html:h5>
																	<html:div class="row cons_opt">
																		<html:div class="col-xs-6">
																			<ImageContent class="pro_img_cont" src="images/ceil_fan.png" description="Ceiling Fan" press="press"></ImageContent>
																		</html:div>
																		<html:div class="col-xs-6">
																			<ImageContent class="pro_img_cont" src="images/led.png" description="LED Tube" press="press"></ImageContent>
																		</html:div>
																	</html:div>
																	<html:div class="row cons_opt">
																		<html:div class="col-xs-6">
																			<ImageContent class="pro_img_cont" src="images/refridg.png" description="Refrigerator" press="press"></ImageContent>
																		</html:div>
																		<html:div class="col-xs-6">
																			<ImageContent class="pro_img_cont" src="images/ac.png" description="AC" press="press"></ImageContent>
																		</html:div>
																	</html:div>
																</html:div>
																<html:div class="col-sm-6">
																	<html:h5>Programmes for Industrial/Commercial Consumers</html:h5>
																	<html:div class="row cons_opt">
																		<html:div class="col-xs-6">
																			<ImageContent class="pro_img_cont" src="images/audit.png" description="Enery Audit" press="press"></ImageContent>
																		</html:div>
																		<html:div class="col-xs-6">
																			<ImageContent class="pro_img_cont" src="images/led.png" description="LED Tube" press="press"></ImageContent>
																		</html:div>
																	</html:div>
																	<html:div class="row cons_opt">
																		<html:div class="col-xs-6">
																			<ImageContent class="pro_img_cont" src="images/ac.png" description="AC" press="press"></ImageContent>
																		</html:div>
																	</html:div>
																</html:div>
															</html:div>
														</html:div>
													</html:div>
												</html:div>
											</html:div>
										</html:div>
									</html:div>
								</Panel>
							</html:div>
						</html:div>
					</html:div>
					<core:Fragment fragmentName="sap.ui.demo.nav.view.Footer" type="XML"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>